{
  "name": "dpos-api-wrapper",
  "version": "1.4.0",
  "description": "DPOS API Wrapper written in TypeScript",
  "main": "dist/es5/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/es5/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/risevision/rise-js-sdk"
  },
  "scripts": {
    "test": "npm run test-all && npm run tslint",
    "test-all": "npm run tslint && npm run test-node && npm run test-browser",
    "tslint": "tslint 'src/**'",
    "test-browser": "karma start karma.conf.js",
    "test-node": "mocha test/",
    "package": "shx rm -rf ./dist/; tsc --outDir dist/es5 --target es5 -d && webpack",
    "coverage": "nyc -e '.ts' -r lcov npm run test-node && cat ./coverage/lcov.info | coveralls",
    "prepublish": "shx rm -rf ./dist/ && npm run package",
    "docs": "shx rm -rf docs ; typedoc -exclude \"**/apis/**\" --theme node_modules/typedoc-clarity-theme/bin --excludePrivate --disableOutputCheck --hideGenerator --module commonjs --mode file --tsconfig ./tsconfig.json  --out docs/ src/; touch docs/.nojekyll "
  },
  "keywords": [
    "rise",
    "lisk",
    "shift",
    "dpos",
    "oxy",
    "ark",
    "cryptocurrency",
    "coin",
    "altcoin"
  ],
  "bugs": {
    "url": "https://github.com/vekexasia/dpos-api-wrapper/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "author": "Andrea Baccega <vekexasia+crypto@gmail.com>",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.0.38",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.29",
    "@types/sinon": "^2.3.1",
    "assert": "1.4.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-typescript": "^3.0.8",
    "karma-webpack": "^2.0.5",
    "mocha": "^3.4.2",
    "nyc": "^11.2.1",
    "proxyquire": "^1.8.0",
    "pug": "^2.0.0-rc.2",
    "shx": "^0.2.2",
    "sinon": "^2.3.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tslib": "^1.7.1",
    "tslint": "^5.8.0",
    "typedoc": "^0.14.2",
    "typedoc-clarity-theme": "^1.1.0",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typescript": "~3.5.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.7.0"
  }
}
